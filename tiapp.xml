<?xml version="1.0" encoding="UTF-8"?>
<ti:app 
  xmlns:ti="http://ti.appcelerator.org">
  <id>smartj.sagan</id>
  <name>SmartJ for Sagan</name>
  <version>1.0.3</version>
  <publisher>Motoi Kataoka</publisher>
  <url>http://motoy3d.blogspot.com</url>
  <description>サガン鳥栖アプリ！</description>
  <copyright>2017 by Motoi Kataoka</copyright>
  <icon>appicon.png</icon>
  <fullscreen>false</fullscreen>
  <navbar-hidden>false</navbar-hidden>
  <analytics>false</analytics>
  <guid>f53d7fd7-cc3a-4f2b-9333-92c940377f75</guid>
  <property name="ti.ui.defaultunit" type="string">dp</property>
  <ios>
    <min-ios-ver>8.0</min-ios-ver>
    <plist>
      <dict>
        <key>UISupportedInterfaceOrientations~iphone</key>
        <array>
          <string>UIInterfaceOrientationPortrait</string>
        </array>
        <key>UIRequiresPersistentWiFi</key>
        <false/>
        <key>UIPrerenderedIcon</key>
        <false/>
        <key>UIStatusBarHidden</key>
        <false/>
        <key>UIStatusBarStyle</key>
        <string>UIStatusBarStyleLightContent</string>
      </dict>
    </plist>
  </ios>
  <iphone>
    <statusbar-hidden>false</statusbar-hidden>
    <fullscreen>false</fullscreen>
    <orientations device="iphone">
      <orientation>Ti.UI.PORTRAIT</orientation>
    </orientations>
    <orientations device="ipad">
      <orientation>Ti.UI.PORTRAIT</orientation>
      <orientation>Ti.UI.UPSIDE_PORTRAIT</orientation>
      <orientation>Ti.UI.LANDSCAPE_LEFT</orientation>
      <orientation>Ti.UI.LANDSCAPE_RIGHT</orientation>
    </orientations>
  </iphone>
  <android 
    xmlns:android="http://schemas.android.com/apk/res/android"/>
    <!-- Android設定はplatform/android/AndroidManifest.xmlで行う。
    WRITE_EXTERNAL_STORAGEパーミッションを無効化するのがtiapp.xmlではできないため -->
    <modules>
      <module platform="android">net.nend</module>
      <module platform="iphone">de.marcelpociot.social</module>
      <module platform="iphone">net.nend</module>
    </modules>
    <deployment-targets>
      <target device="android">true</target>
      <target device="ipad">false</target>
      <target device="iphone">true</target>
      <target device="mobileweb">false</target>
    </deployment-targets>
    <sdk-version>5.5.1.GA</sdk-version>
    <property name="appc-app-id" type="string">58bbb4e6b1de20f5473b5ca7</property>
  </ti:app>